{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "7.5.0",
        "laravel/framework": "9.43.0",
        "laravel/sanctum": "3.0.1",
        "laravel/tinker": "2.7.3",
        "league/glide-laravel": "1.0.0",
        "tonysm/tailwindcss-laravel": "0.8.0"
    },
    "require-dev": {
        "deployer/deployer": "7.0.2",
        "fakerphp/faker": "1.20.0",
        "laravel/pint": "1.2.1",
        "laravel/sail": "1.16.3",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "6.3.1",
        "phpmd/phpmd": "2.13.0",
        "phpunit/phpunit": "9.5.27",
        "spatie/laravel-ignition": "1.6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpmd": [
            "@php vendor/bin/phpmd app/ text phpmd.xml.dist"
        ],
        "codestyle": [
            "@php vendor/bin/pint"
        ],
        "check-all": [
            "@codestyle",
            "@phpmd"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
